<!--[if (gte IE 9)|!(IE)]><!-->
<html lang="en" class="no-js" {{function('language_attributes')}}>
    <!--<![endif]-->

    <head>

        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"/>
        <link rel="profile" href="http://gmpg.org/xfn/11">
        <script>(function(html){html.className = html.className.replace(/\bno-js\b/,'js')})(document.documentElement);</script>

        <title>{{function('wp_title', '|', true, 'right')}}</title>

        {# add stylesheets #}
        {# {% do add_stylesheet('bootstrap', template_uri ~ '/assets/css/vendor/bootstrap.css', [], '3.2.0', 'all') %} #}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

        {# add scripts #}
        {# {% do add_script_file('vendor', template_uri ~ '/assets/built/vendor.built.js', [], '2.1.14', false) %} #}

        {{wp_head}}

        <link rel="alternate" type="application/rss+xml" title="{{function('get_bloginfo', 'name')}} Feed" href="{{function('get_feed_link')}}">

        <!--[if lt IE 9]>
            <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
            <script>window.html5 || document.write('<script src="/themes/mmf/javascript/vendor/html5shiv.js"><\/script>')</script>
        <![endif]-->

    </head>

    <body {{function('body_class')}}>

        <div id="page">

            <div id="wrapper">

                {# declare header block #}
                {% block header %}
                    {% include 'includes/header.html.twig' %}

                    {# TIMBER SEARCH FORM #}
                    {#{%  include 'includes/search-form.html.twig' %}#}
                {% endblock %}

                {# block body block #}
                {% block body %}{% endblock %}

                <div class="push"></div>

            </div>
            <!-- end div#wrapper -->


            {# block footer block #}
            {% block footer %}
                {% include 'includes/footer.html.twig' %}
            {% endblock %}

        </div>
        <!-- end div#page -->

        {{wp_footer}}

    </body>

</html>